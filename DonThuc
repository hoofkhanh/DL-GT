#include<stdio.h>

struct DonThuc{
	float a;
	int n;
};

typedef struct DonThuc DT;
void nhap(DT &);
void xuat(DT);
DT tinhtichDonThuc(DT);

void nhap(DT &dt){
	printf("nhap a");
	scanf("%f",&dt.a);
	printf("nhap n");
	scanf("%d",&dt.n);
}

void xuat(DT dt){
	printf("%.2fx^%d",dt.a,dt.n);
}



int main(){
	DT d;
	nhap(d);
	xuat(d);
	
}
